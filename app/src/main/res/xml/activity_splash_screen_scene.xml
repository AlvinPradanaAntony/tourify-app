<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="5000"
        motion:autoTransition="animateToEnd"
        motion:motionInterpolator="easeInOut">
        <KeyFrameSet>
            <KeyAttribute
                motion:motionTarget="@+id/first_background"
                motion:framePosition="0"
                android:scaleX="50"
                android:scaleY="50" />
            <KeyAttribute
                motion:motionTarget="@+id/first_background"
                motion:framePosition="65"
                android:scaleX="0"
                android:scaleY="0" />
            <KeyAttribute
                motion:motionTarget="@+id/logo_app"
                motion:framePosition="0" >
                <CustomAttribute
                    motion:customFloatValue="0"
                    motion:attributeName="crossfade" />
            </KeyAttribute>
            <KeyAttribute
                motion:motionTarget="@+id/logo_app"
                motion:framePosition="68" >
                <CustomAttribute
                    motion:customFloatValue="1"
                    motion:attributeName="crossfade" />
            </KeyAttribute>
            <KeyAttribute
                motion:motionTarget="@+id/logo_app"
                motion:framePosition="54" >
                <CustomAttribute
                    motion:attributeName="crossfade"
                    motion:customFloatValue="0" />
            </KeyAttribute>
            <KeyAttribute
                motion:motionTarget="@+id/logo_app"
                motion:framePosition="0" />
            <KeyAttribute
                motion:motionTarget="@+id/logo_app"
                motion:framePosition="0"
                android:alpha="0" />
            <KeyAttribute
                motion:motionTarget="@+id/logo_app"
                motion:framePosition="4"
                android:alpha="1" />
            <KeyAttribute
                motion:motionTarget="@+id/logo_app"
                motion:framePosition="0"
                android:scaleX="0"
                android:scaleY="0" />
            <KeyAttribute
                motion:motionTarget="@+id/logo_app"
                motion:framePosition="4"
                android:scaleX="1.2"
                android:scaleY="1.2" />
            <KeyAttribute
                motion:motionTarget="@+id/logo_app"
                motion:framePosition="8"
                android:scaleX="1"
                android:scaleY="1" />
            <KeyAttribute
                motion:motionTarget="@+id/title_logo"
                motion:framePosition="0"
                android:alpha="0" />
            <KeyAttribute
                motion:motionTarget="@+id/txt_slogan"
                motion:framePosition="0"
                android:alpha="0" />
            <KeyAttribute
                motion:motionTarget="@+id/title_logo"
                motion:framePosition="14"
                android:alpha="0" />
            <KeyAttribute
                motion:motionTarget="@+id/title_logo"
                motion:framePosition="20"
                android:alpha="1" />
            <KeyAttribute
                motion:motionTarget="@+id/txt_slogan"
                motion:framePosition="24"
                android:alpha="0" />
            <KeyAttribute
                motion:motionTarget="@+id/txt_slogan"
                motion:framePosition="38"
                android:alpha="1" />
            <KeyAttribute
                motion:motionTarget="@+id/logo_app"
                motion:framePosition="22"
                android:translationY="-40dp" />
            <KeyAttribute
                motion:motionTarget="@+id/logo_app"
                motion:framePosition="14"
                android:translationY="0dp" />
            <KeyAttribute
                motion:motionTarget="@+id/logo_app"
                motion:framePosition="100"
                android:translationY="-40dp" />
            <KeyAttribute
                motion:motionTarget="@+id/title_logo"
                motion:framePosition="100"
                android:translationY="-42dp" />
            <KeyAttribute
                motion:motionTarget="@+id/title_logo"
                motion:framePosition="14"
                android:translationY="-5dp" />
            <KeyAttribute
                motion:motionTarget="@+id/title_logo"
                motion:framePosition="24"
                android:translationY="-42dp" />
            <KeyAttribute
                android:scaleX="50"
                android:scaleY="50"
                motion:motionTarget="@+id/first_background"
                motion:framePosition="24" />
            <KeyAttribute
                motion:motionTarget="@+id/txt_slogan"
                motion:framePosition="54" >
                <CustomAttribute
                    motion:attributeName="textColor"
                    motion:customColorValue="#FFFFFF" />
            </KeyAttribute>
            <KeyAttribute
                motion:motionTarget="@+id/txt_slogan"
                motion:framePosition="65" >
                <CustomAttribute
                    motion:attributeName="textColor"
                    motion:customColorValue="#06093D" />
            </KeyAttribute>
            <KeyAttribute
                motion:motionTarget="@+id/progressBar"
                motion:framePosition="75"
                android:alpha="0" />
            <KeyAttribute
                motion:motionTarget="@+id/progressBar"
                motion:framePosition="85"
                android:alpha="1" />
            <KeyAttribute
                motion:motionTarget="@+id/progressBar"
                motion:framePosition="99"
                android:alpha="1" />
            <KeyAttribute
                motion:motionTarget="@+id/progressBar"
                motion:framePosition="100"
                android:alpha="0" />
        </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_width="128dp"
            android:layout_height="128dp"
            android:id="@+id/first_background"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:scaleX="50"
            android:scaleY="50" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/logo_app"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/abstract1"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent">
            <CustomAttribute
                motion:attributeName="alpha"
                motion:customFloatValue="0.2" />
        </Constraint>
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/title_logo"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/logo_app"
            android:layout_marginTop="8dp"/>
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/txt_slogan"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.5700">
            <CustomAttribute
                motion:attributeName="textColor"
                motion:customColorValue="#FFFFFF" />
        </Constraint>
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/progressBar"
            android:layout_width="20dp"
            android:layout_height="20dp"
            motion:layout_constraintVertical_bias="0.9"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/logo_app"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent" >
            <CustomAttribute
                motion:attributeName="crossfade"
                motion:customFloatValue="1" />
        </Constraint>
        <Constraint
            android:layout_width="128dp"
            android:layout_height="128dp"
            android:id="@+id/first_background"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:scaleX="0"
            android:scaleY="0" />
        <Constraint
            android:id="@+id/abstract1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_editor_absoluteY="42dp" >
            <CustomAttribute
                motion:attributeName="alpha"
                motion:customFloatValue="0.2" />
        </Constraint>
        <Constraint
            android:id="@+id/txt_slogan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <CustomAttribute
                motion:attributeName="textColor"
                motion:customColorValue="#06093D" />
        </Constraint>
        <Constraint
            android:id="@+id/title_logo"
            android:layout_width="wrap_content"
            android:layout_height="0dp"/>
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/progressBar"
            android:layout_width="20dp"
            android:layout_height="20dp"
            motion:layout_constraintVertical_bias="0.9"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent" />
    </ConstraintSet>
</MotionScene>