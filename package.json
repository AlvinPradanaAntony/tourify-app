    "url": "https://github.com/Bangkit-C2-PS424-Team/tourify-app/issues"
  },
  "homepage": "https://github.com/Bangkit-C2-PS424-Team/tourify-app#readme",
  "dependencies": {
    "@tensorflow/tfjs": "^4.7.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
