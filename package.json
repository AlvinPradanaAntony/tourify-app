{
  "name": "tourify-app-api",
  "version": "1.0.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "./src/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "babel-node .",
    "serve": "nodemon --exec node_modules/.bin/babel-node .",
    "build": "babel src -d build --copy-files",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "migrate-undo": "npx sequelize db:migrate:undo:all",
    "seed-undo": "npx sequelize db:seed:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bangkit-C2-PS424-Team/tourify-app.git"
  },
